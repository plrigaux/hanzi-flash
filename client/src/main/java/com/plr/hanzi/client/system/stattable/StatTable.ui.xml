<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d='urn:import:com.plr.hanzi.client.view.definition'
	xmlns:w="urn:import:com.plr.hanzi.client.view.welcome" xmlns:c='urn:import:com.google.gwt.user.cellview.client'>

	<ui:style>
		.scrollable {
			height: 400px;
			width: 160px;
			border: 1px solid #ccc;
			text-align: left;
		}
		
		.contactFormCell {
			padding-left: 20px;
		}
		
		.panel {
			margin: 0px auto;
		}
	</ui:style>

	<g:HTMLPanel>

		<c:AbstractPager ui:field='pager' addStyleNames='{style.panel}' />
		<c:CellTable addStyleNames='{style.scrollable} {style.panel}'
			ui:field='cellTable' />

		<!-- <s:ZwCharDetails ui:field='contactForm' /> -->

	</g:HTMLPanel>
</ui:UiBinder> 